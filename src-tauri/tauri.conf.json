{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "sn",
  "version": "0.1.0",
  "identifier": "com.sidnezsbo.sn",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "menubar",
        "width": 280,
        "height": 400,
        "visible": false,
        "hiddenTitle": true,
        "titleBarStyle": "Overlay",
        "alwaysOnTop": true,
        "closable": false,
        "minimizable": false,
        "decorations": false,
        "focus": false,
        "transparent": true
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null,
      "capabilities": []
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
